abstract class Animal
{       
    protected string Name;
    protected string Type; 
      
    public abstract void Run();
    public abstract void Fly();
    public abstract void Sound();

    public void GetInfo()
    {
        Console.WriteLine($"ë„ˆì˜ ì´ë¦„ì€ {Name}, ì¢…ë¥˜ëŠ” {Type}!!");
    }  
}

class Dog : Animal
{
