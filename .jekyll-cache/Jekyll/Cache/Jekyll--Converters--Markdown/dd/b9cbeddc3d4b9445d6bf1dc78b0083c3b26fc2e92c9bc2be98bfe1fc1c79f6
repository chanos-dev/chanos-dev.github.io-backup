<div class="language-plaintext highlighter-rouge"><div class="highlight"><pre class="highlight"><code>class Program
{
    static void Main(string[] args)
    { 
        Dragon dragon = new Dragon("ìš©ìš©ì´", "ì•„ì´ìŠ¤ë“œë˜ê³¤");
        dragon.Breath();
        dragon.MoveFly();
    }
}

interface IAttack
{ 
    void Breath();
}

interface IMove
{
    void MoveGround();
    void MoveFly();
}

class Dragon : IAttack, IMove
{
    private string name;
    private string type; 

    public Dragon(string name, string type)
    {
        this.name = name; 
        this.type = type;
    } 

    public void Breath()
    {
        Console.WriteLine($"{this.name}ì˜ ê³µê²©!");
    }

    public void MoveFly()
    {
        Console.WriteLine($"{this.name}ê°€ ë•…ì—ì„œ ì´ë™");
    }

    public void MoveGround()
    {
        Console.WriteLine($"{this.name}ê°€ í•˜ëŠ˜ì—ì„œ ì´ë™");
    }
}
