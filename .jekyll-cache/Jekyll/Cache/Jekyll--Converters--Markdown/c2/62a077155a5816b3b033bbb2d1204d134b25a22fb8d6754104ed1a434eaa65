<span class="p">{</span>
    <span class="k">static</span> <span class="k">void</span> <span class="nf">Main</span><span class="p">(</span><span class="kt">string</span><span class="p">[]</span> <span class="n">args</span><span class="p">)</span>
    <span class="p">{</span> 
        <span class="n">Dragon</span> <span class="n">dragon</span> <span class="p">=</span> <span class="k">new</span> <span class="nf">Dragon</span><span class="p">(</span><span class="s">"ìš©ìš©ì´"</span><span class="p">,</span> <span class="s">"ì•„ì´ìŠ¤ë“œë˜ê³¤"</span><span class="p">);</span>
